{
  "expo": {
    "name": "RouteCastle",
    "slug": "routecastle",
    "version": "1.0.0",
    "orientation": "portrait",
    "sdkVersion": "50.0.0",
    "platforms": ["android"],
    "jsEngine": "hermes",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
